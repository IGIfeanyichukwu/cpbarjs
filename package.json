{
  "name": "cyprobar",
  "version": "1.0.2",
  "description": "Cyprobar is a javascript library for seamlessly creating circular progress bars",
  "main": "dist/cyprobar.js",
  "bin": "dist/cyprobar.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "parcel build -o cyprobar.js --global CPB --no-source-maps ./src/js/cyprobar.js",
